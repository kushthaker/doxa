   "source": [
    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "getx = lambda x: 4*x*(1 - x) "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "test_range = np.arange(0, 1000)\n",
    "x = 10\n",
    "xs = []\n",
    "for _ in test_range:\n",
    "  x = getx(x)\n",
    "  xs.append(x)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "plt.plot(test_range, xs)"
   ]
